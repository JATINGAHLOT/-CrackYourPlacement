
class solution {
  public:
    long long multiplyTwoLists(Node *first, Node *second) {
     
    long long num1 = 0, num2 = 0, mod = 1000000007;

    while (first) {
        num1 = (num1 * 10 + first->data) % mod;
        first = first->next;
    }

    while (second) {
        num2 = (num2 * 10 + second->data) % mod;
        second = second->next;
    }

    long long result = (num1 * num2) % mod;
    return result;
    }
};
